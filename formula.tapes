let A : sort.   let B : sort.   let C : sort.
let f : A -> B * C.

set (otimesdist : 0.5). // for now these have to be float
set (oplusdist : 0.25).

let t1 : term = copy(A + B + C) ; cocopy(A + B + C) ; (id(A) + gen(f, B, A) + gen(f, C, A)) ; (id(A) + join(A)).

check t1.

draw t1 to "./result_examples/distributor"
